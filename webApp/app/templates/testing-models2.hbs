  {{#each model.assigns as |assign|}}
    {{#forms/assign-data data=assign as |data|}}
      <button {{action 'submitRecord' data}}> Submit </button>
      <button {{action 'deleteRecord' data}}> Delete </button>
    {{/forms/assign-data}}
  {{/each}}

  {{#each model.coursePrefixes as |prefix|}}
    {{#forms/course-prefix-data data=prefix as |data|}}
      <button {{action 'submitRecord' data}}> Submit </button>
      <button {{action 'deleteRecord' data}}> Delete </button>
    {{/forms/course-prefix-data}}
  {{/each}}
{{outlet}}