{{#lf-form rules=rules}}

{{lf-input

	name="CourseId"
	label="Course ID"
	property=data.courseId
	on-update=(action (mut data.courseId))
	validate=validate
}}

{{lf-input
    name="instructorId"
    label="instructor Id"
    property=data.instructorId
    on-update=(action (mut data.instructorId))
    validate=validate
  }}


{{lf-input
    label="Semester Id"
    name="semesterId"
    property=data.semesterId
    on-update=(action (mut data.semesterId))
    validate=validate
  }}



{{lf-input

	label="section Number"
	property=data.sectionNum
	name="sectionNum"
	validate=validate
	on-update=(action (mut data.sectionNum))
}}


{{lf-input
	name="Locked"
	label="locked"
	property=data.locked
	validate=validate
	on-update=(action (mut data.locked))
}}

{{lf-input
	name="numStudents"
	label="Number Students"
	property=data.numStudents
	validate=validate
	on-update=(action (mut data.numStudents))
}}


{{/lf-form}}
{{yield data}}
